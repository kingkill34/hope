<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.inng.haircut.dao.TestUserDetailDao"> 

  
    <!-- 获取用户集合 -->
    <select id="getList" resultType="testUserDetail"> 
      select id,
      		 username,
      		 age,
      		 sex,
      		 phone,
      		 addDate,
      		 success 
      		 from tb_user_detail
       <!-- 这里sql结尾不能加分号，否则报“ORA-00911”的错误 --> 
    </select> 
    
    <insert id="add">
        insert into tb_user_detail set userName=#{userName},age=${age},sex=${sex},phone=${phone},addDate=${addDate}
        
    </insert>
     
</mapper>